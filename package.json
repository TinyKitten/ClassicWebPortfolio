{
  "name": "tinykitten",
  "version": "1.0.0",
  "description": "TinyKitten Official Website",
  "author": "TinyKitten <catwalk8205@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "format": "prettier --trailing-comma=es5 --single-quote --write \"./**/*.{js,vue}\"",
    "pre": "concurrent \"npm run format\" \"npm run lint\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre",
      "pre-push": "npm run pre"
    }
  },
  "dependencies": {
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "concurrently": "^3.5.1",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^0.14.3",
    "prettier": "1.11.1"
  }
}
